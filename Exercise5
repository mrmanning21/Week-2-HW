public class FloatEqu {
	/*-----------------------------------------------------------------

	 	A program that determines if two values are equal.

	  -----------------------------------------------------------------*/

	public static void main(String[] args) {
		
		double randOne = (1.0/10) * (1.0/10), randTwo = (1.0/100); //inserting variables and math

		if (randOne == randTwo)
			System.out.println("EQUAL");
		else
			System.out.println("NOT EQUAL");
	}

}
